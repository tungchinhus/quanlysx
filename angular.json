{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "quanlysx": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/public",
            "baseHref": "/quanlysx/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "src/resources/",
                "output": "/resources"
              }
            ],
            "styles": [
              "src/styles.scss",
              "src/themes/theme.scss",
              "src/themes/icons.scss",
              "node_modules/bootstrap/scss/bootstrap.scss"
            ],
            "scripts": [{
              "inject": true,
              "bundleName": "moment",
              "input": "node_modules/moment/min/moment.min.js"
            }, {
              "inject": true,
              "bundleName": "jquery",
              "input": "node_modules/jquery/dist/jquery.min.js"
            }, {
              "inject": true,
              "bundleName": "lodash",
              "input": "node_modules/lodash/lodash.js"
            }, {
              "inject": true,
              "bundleName": "html2canvas",
              "input": "node_modules/html2canvas/dist/html2canvas.min.js"
            }],
            "allowedCommonJsDependencies": [
              "lodash",
              "@rsf/rsf-angular-base/dist/config.service",
              "file-saver",
              "raf",
              "core-js",
              "html2canvas",
              "rgbcolor"
            ]
          },
          "configurations": {
            "production": {
              "baseHref": "/quanlysx/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb",
                  "maximumError": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "commonChunk": true,
              "sourceMap": false,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "pre-prod": {
              "baseHref": "/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb",
                  "maximumError": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "commonChunk": true,
              "sourceMap": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "non-prod": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb",
                  "maximumError": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "commonChunk": true,
              "sourceMap": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "quanlysx:build:production"
            },
            "development": {
              "browserTarget": "quanlysx:build:development",
              "port": 4200,
              "host": "localhost"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "quanlysx:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "src/resources/",
                "output": "/resources"
              }
            ],
            "styles": ["src/styles.scss", "src/themes/theme.scss", "src/themes/icons.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
